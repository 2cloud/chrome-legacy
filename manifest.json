{
  "name": "2cloud",
  "version": "2.0.5.1",
  "minimum_chrome_version": "5.0.316.0",
  "description": "The Chrome extension for the 2cloud Project. Send and receive links from any client supported by the project.",
  "permissions": [
    "tabs",
    "<all_urls>",
    "*://*/*"
  ],
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "background_page": "background.html",
  "options_page": "config.html",
  "homepage_url": "http://links.2cloudproject.com/chrome",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
