{
  "name": "2cloud",
  "version": "2.0.5.1",
  "description": "The Chrome extension for the 2cloud Project. Send and receive links from any client supported by the project.",
  "permissions": [
  	"tabs",
        "notifications",
    "<all_urls>",
    "*://*/*"
  ],
  "background_page": "background.html",
  "options_page": "config.html",
  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png" }
}
