{
  "name": "android2cloud",
  "version": "1.0.1.1",
  "description": "A way to push content from your Android phone to your Chrome web browser.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "android2cloud",
    "default_popup": "send_link.html"
  },
  "permissions": [
  	"tabs",
        "notifications",
    "<all_urls>"
  ],
  "background_page": "background.html",
  "options_page": "config.html",
  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png" }
}