{
  "name": "android2cloud",
  "version": "2.0.3",
  "description": "A way to push content from your Android phone to your Chrome web browser.",
  "permissions": [
  	"tabs",
        "notifications",
    "<all_urls>",
    "*://*/*"
  ],
  "background_page": "background.html",
  "options_page": "config.html",
  "icons": { "16": "icon.png",
             "48": "icon48.png",
            "128": "icon128.png" }
}